{
  "name": "find-old-todos",
  "version": "0.0.1",
  "description": "",
  "keywords": [
    "todo",
    "todos"
  ],
  "homepage": "https://github.com/onderonur/find-old-todos",
  "bugs": {
    "url": "https://github.com/onderonur/find-old-todos/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onderonur/find-old-todos.git"
  },
  "license": "MIT",
  "author": "onderonur",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "find-old-todos": "bin/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "attw": "attw --pack .",
    "build": "tsup",
    "codequality": "npm run format && npm run lint && npm run typecheck",
    "codequality:fix": "npm run format:fix && npm run lint:fix && npm run typecheck",
    "dev": "tsx watch src/index.ts",
    "format": "prettier --check --ignore-unknown .",
    "format:fix": "prettier --write --ignore-unknown .",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --max-warnings 0 --fix",
    "prepublishOnly": "npm run build",
    "publint": "publint",
    "typecheck": "tsc",
    "unused": "knip",
    "updates": "npm-check-updates -i"
  },
  "dependencies": {
    "@typescript-eslint/typescript-estree": "^8.31.1",
    "arg": "^5.0.2",
    "dayjs": "^1.11.13",
    "ora": "^8.2.0",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.4",
    "@eslint/js": "^9.26.0",
    "@types/node": "^22.15.3",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-unicorn": "^59.0.0",
    "globals": "^16.0.0",
    "knip": "^5.53.0",
    "npm-check-updates": "^18.0.1",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.10",
    "publint": "^0.3.12",
    "tsup": "^8.4.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1"
  },
  "packageManager": "pnpm@10.10.0"
}
